{
  "name": "experimental-runtime",
  "version": "0.1.0",
  "description": "An experimental runtime environment for JavaScript.",
  "main": "dist/experimental-runtime.js",
  "scripts": {
    "test": "npm run build && mocha -s 0",
    "prettier": "prettier {*.*,**/*.*} --single-quote --write",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/experimental-runtime.git"
  },
  "keywords": [
    "Runtime",
    "JavaScript",
    "Sandbox",
    "REPL"
  ],
  "author": "Curran Kelleher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vizhub-core/experimental-runtime/issues"
  },
  "homepage": "https://github.com/vizhub-core/experimental-runtime#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "puppeteer": "^13.4.1",
    "rollup": "^2.69.2"
  }
}
